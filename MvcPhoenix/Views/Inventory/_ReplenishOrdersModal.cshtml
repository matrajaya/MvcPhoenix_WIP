@model MvcPhoenix.Models.Inventory
@using MvcPhoenix.Models
@{}

<div class="row">
    <div class="col-md-12 text-center">
        <h3 class="modal-title">Replenishment Order Details</h3>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-md-6 form-group">
        <label>Order Number</label>
        <input class="form-control" />
    </div>
    <div class="col-md-6 form-group">
        <label>Order Date</label>
        <input type="date" class="form-control" />
    </div>
</div>
<div class="row">
    <div class="col-md-6 form-group">
        <label>Supplyid</label>
        <input class="form-control" />
    </div>
    <div class="col-md-3 form-group">
        <label>Quantity</label>
        <input class="form-control" />
    </div>
    <div class="col-md-3 form-group">
        <label>Size</label>
        <input class="form-control" />
    </div>
</div>
<div class="row">
    <div class="col-md-6 form-group">
        <label>Status</label>
        <input class="form-control" />
    </div>
    <div class="col-md-6 form-group">
        <label>ETA</label>
        <input type="date" class="form-control" />
    </div>
</div>
<div class="row">
    <div class="col-md-12 form-group">
        <label>Notes</label>
        <textarea class="form-control" rows="3"></textarea>
    </div>
</div>
<div class="row">
    <div class="col-md-6 form-group">
        <input class="btn btn-success btn-block" type="button" value="Save" />
    </div>
    <div class="col-md-6 form-group">
        <input class="btn btn-danger btn-block" data-dismiss="modal" type="button" value="Close" />
    </div>
</div>


