@using GleamTech.Web.Mvc
@using GleamTech.FileUltimate
@model FileManager

@{
    ViewBag.Title = "File Management";
}

@Html.RenderCss(Model)
@Html.RenderJs(Model)

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <img src="~/Content/images/logos/oltlogo.jpg" style="width: auto; height: 73px; float: right" alt="acme logo">
        </div>
        <div class="col-lg-12">
            <h1 class="page-header">
                @ViewBag.Title
            </h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            @Html.RenderControl(Model)
        </div>
    </div>
    
</div>