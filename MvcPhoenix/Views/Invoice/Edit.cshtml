                                            @Html.TextBoxFor(m => m.totalfreight, new { @class = "pull-right", @style = "width: 100px", @data_cell = "B1", disabled = "disabled" })
                                            @Html.HiddenFor(m => m.totalfreight)
                                            @Html.ValidationMessageFor(m => m.totalfreight)
                                        </td>
                                    </tr>
                                    <tr>
                                        <th>
                                            Freight Harzardous Surcharge
