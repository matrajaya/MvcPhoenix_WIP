            C8: { formula: 'A8*B8' },
            C9: { formula: 'A9*B9' },
            C10: { formula: 'A10*B10' },
            C11: { formula: 'A11*B11' },
            C17: { formula: 'A17*B17' },
            C18: { formula: 'A18*B18' },
            C20: { formula: 'A20*B20' },
            C24: { formula: 'SUM(C1:C23)' }
        }
    })

    $('#verificationcalc').calx({
        data: {
            A1: { formula: '#masterinvoicecalc!E1' },
            B1: { formula: '#billingworksheetcalc!C24' },
            C1: { formula: 'A1-B1' }
        }
    })
