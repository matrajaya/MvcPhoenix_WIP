                                    <label><abbr title="Total Due minus Grand Total.&#13;This number should be 0.">Totals</abbr> Balance</label>
                                    <input id="totalsdiff" class="form-control input-sm" readonly style="width: 100px" data-cell="C1">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
