                                        </th>
                                        <td>
                                            @Html.TextBoxFor(m => m.totalcostsamples, new { @class = "pull-right", @style = "width: 100px", @data_cell = "A1", disabled = "disabled" })
                                            @Html.HiddenFor(m => m.totalcostsamples)
