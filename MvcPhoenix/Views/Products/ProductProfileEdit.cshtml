@model MvcPhoenix.Models.ProductProfile
@{
    ViewBag.Title = "Product Profile:";
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <img src="@Model.logofilename" style="width: auto; height: 73px; float: right" alt="@Model.clientname logo">
        </div>
        <div class="col-lg-12">
            <h1 class="page-header">
                @ViewBag.Title  @Model.productcode
            </h1>
        </div>
    </div>
    <!-- /.Page Heading -->
    @using (Ajax.BeginForm("SaveProductProfile", "Products",
    new AjaxOptions
    {
        OnBegin = "$('#dvSaveResult').html('')",
        HttpMethod = "POST",
        UpdateTargetId = "dvSaveResult",
        LoadingElementId = "dvspinner",
        InsertionMode = InsertionMode.Replace
    }))
    {

    <!-- do not remove -->
        @Html.HiddenFor(m => m.productmasterid)
        @Html.HiddenFor(m => m.clientid)
        @Html.HiddenFor(m => m.productdetailid)

        @*<div class="row">
                <div class="form-group col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-search fa-fw"></i></span>
                        <input type="search" class="form-control" placeholder="Type Product Code...">
                        <span class="input-group-btn"><input type="reset" class="btn btn-success" value="Search" /></span>
                    </div>
                    <hr />
                </div>
            </div>*@

        <div class="form-group row">
            <div class="col-lg-12">
                <i class="fa fa-arrow-circle-left"></i> @Html.ActionLink("Back to Products", "Index")
                @*<span id="dvTopMessage" class="pull-right" style="">@Html.Raw(@Model.UpdateResult)</span>*@<!-- show message after save -->
                @*<label id="discontinued" class="col-md-12 collapse form-control" style="background-color: red; color: white"> Discontinued Product</label>*@
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#prodid"><i class="fa fa-suitcase fa-fw"></i> Product Identification</h3>
            </div>
            <div id="prodid" class="panel-body">
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Client</label>
                        @Html.TextBoxFor(m => m.clientname, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group col-md-6">
                        <label>Business Unit / Division</label>
                        @Html.DropDownListFor(m => m.masterdivisionid, Model.ListOfDivisions, new { @class = "form-control" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>Equivalence</label><br />
                        <textarea rows="5" class="form-control" readonly="readonly">
                            @for (int i = 0; i < Model.ListOfEquivalents.Count(); i++)
                            {
                                @Model.ListOfEquivalents[i];
                            }
                        </textarea>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Cross Reference List</label><br />
                        <textarea rows="5" class="form-control" readonly="readonly">
                            @for (int i = 0; i < Model.ListOfProductCodesXRefs.Count(); i++)
                            {
                                @Model.ListOfProductCodesXRefs[i];
                            }
                        </textarea>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Profile Revision Date</label>
                        @Html.TextBoxFor(m => m.sgrevisiondate, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group col-md-3">
                        <label>MSDS Revision Date</label>
                        @Html.TextBoxFor(m => m.msdsrevisiondate, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group  col-md-3">
                        <label>Label Revision Date</label>
                        @Html.TextBoxFor(m => m.labelrevisiondate, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group col-md-3">
                        <label>@Html.CheckBox("globalproduct", Model.globalproduct != null && (bool)Model.globalproduct) Global Product</label><br />
                        <label>@Html.CheckBox("accuracyverified", Model.accuracyverified != null && (bool)Model.accuracyverified) Accuracy Verified</label><br />
                        <!--Todo: Creat condition at target if dicontinued is true then show-->
                        @Html.CheckBox("discontinued", Model.discontinued != null && (bool)Model.discontinued)
                        <label data-toggle="collapse" data-target="#discontinued">Discontinued</label>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Product Code</label>
                        @Html.TextBoxFor(m => m.productcode, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-6">
                        <label>Product Name</label>
                        @Html.TextBoxFor(m => m.productname, new { @class = "form-control" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-2">
                        <label>Shelf Life</label>
                        @Html.TextBoxFor(m => m.shlflife, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-2">
                        <label>Hazard Class</label>
                        @Html.TextBoxFor(m => m.hazardclassground_sg, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-2">
                        <label>S.G.</label>
                        @Html.TextBoxFor(m => m.specificgravity, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-2">
                        <label>Density LBS/GAL</label>
                        @Html.TextBoxFor(m => m.density, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-2">
                        <label>pH</label>
                        @Html.TextBoxFor(m => m.phvalue, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-2">
                        <label>Restricted Qty (lbs)</label>
                        @Html.TextBoxFor(m => m.restrictedamount, new { @class = "form-control" })
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-md-12">
                        <label><a href="#" onclick="alert('TODO Build, TODO')">+ Add Package Part Number</a></label>
                    </div>
                    <div class="table-responsive col-md-12">
                        <table class="table table-hover table-striped">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>Part Number</th>
                                    <th>Size</th>
                                    <th>Weight</th>
                                    <th>Package</th>
                                    <th>DOT Spec</th>
                                    <th>UN Spec</th>
                                    <th>Torque</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                    <td>CMC-84</td>
                                    <td>QT</td>
                                    <td></td>
                                    <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                    <td>2E</td>
                                    <td>1H2/Y23/S</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                    <td>CMC-84</td>
                                    <td>GAL</td>
                                    <td></td>
                                    <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                    <td>2E</td>
                                    <td>1H2/Y23/S</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                    <td>CMC-84</td>
                                    <td>5GAL</td>
                                    <td></td>
                                    <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                    <td>2E</td>
                                    <td>1H2/Y23/S</td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="col-md-12">
                        <label><a href="#" onclick="alert('TODO Build, TODO')">+ Add Size Information</a></label>
                    </div>
                    <div class="table-responsive col-md-12">
                        <table class="table table-hover table-striped">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>Size</th>
                                    <th>Mnemonic</th>
                                    <th>BusArea</th>
                                    <th>Warehouse</th>
                                    <th>Bin</th>
                                    <th>Min Qty</th>
                                    <th>Max Qty</th>
                                    <th>Re-order Qty</th>
                                    <th>Package</th>
                                    <th>Gnd Haz</th>
                                    <th>Air Hzd</th>
                                    <th>Notes</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.ListOfShelfItems)
                                {
                                    <tr>
                                        <td><a href="#" onclick="alert('Build Object, Modal delete confirm')"><i class="fa fa-minus-circle fa-fw" style="color: red"></i></a></td>
                                        <td><strong>@item.size</strong></td>
                                        <td>@item.mnemonic</td>
                                        <td>@item.busarea</td>
                                        <td>@item.warehouse</td>
                                        <td>@item.bin</td>
                                        <td>@item.reordermin.ToString()</td>
                                        <td>@item.reordermax.ToString()</td>
                                        <td>@item.reorderqty.ToString()</td>
                                        <td>@item.package</td>
                                        <td>@item.groundhazard</td>
                                        <td>@item.airhazard</td>
                                        <td>@item.notes</td>
                                        <td><a href="#" onclick="alert('Build object, Modal edit popup')">Edit</a></td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#special"><i class="fa fa-suitcase fa-fw"></i> Special Handling & Storage</h3>
            </div>
            <div id="special" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-3">
                        <p>@Html.CheckBox("globalproduct", Model.globalproduct != null && (bool)Model.globalproduct) Global Product</p>
                        <p>@Html.CheckBox("cleanroomgmp", Model.cleanroomgmp != null && (bool)Model.cleanroomgmp) Clean Room GMP</p>
                        <p>@Html.CheckBox("nitrogenblanket", Model.nitrogenblanket != null && (bool)Model.nitrogenblanket) Nitrogen Blanket</p>
                        <p>@Html.CheckBox("moisturesensitive", Model.moisturesensitive != null && (bool)Model.moisturesensitive) Moisture Sensitive</p>
                    </div>
                    <div class="form-group col-md-3">
                        <p>@Html.CheckBox("mixwell", Model.mixwell != null && (bool)Model.mixwell) Mix Well</p>
                        <p>@Html.CheckBox("donotpackabove60", Model.donotpackabove60 != null && (bool)Model.donotpackabove60) Don't Pack Above 60% RH</p>
                        <p>@Html.CheckBox("flammableground", Model.flammableground != null && (bool)Model.flammableground) Flammable Ground</p>
                        <p>Torque: @Html.TextBoxFor(m => m.torquerequirements)</p>
                    </div>
                    <div class="form-group col-md-3">
                        <p>Flash Point: @Html.TextBoxFor(m => m.flashpoint)</p>
                        <p>@Html.CheckBox("specialblend", Model.specialblend != null && (bool)Model.specialblend) Special Blend</p>
                        <p>@Html.CheckBox("heatpriortofilling", Model.heatpriortofilling != null && (bool)Model.heatpriortofilling) Heat Prior to Filing</p>
                        <p>@Html.CheckBox("freezerstorage", Model.freezerstorage != null && (bool)Model.freezerstorage) Freezer Storage</p>
                    </div>
                    <div class="form-group col-md-3">
                        <p>@Html.CheckBox("refrigerate", Model.refrigerate != null && (bool)Model.refrigerate) Refrig Storage</p>
                        <p>@Html.CheckBox("flammablestorageroom", Model.flammablestorageroom != null && (bool)Model.flammablestorageroom) Flammable Storage</p>
                        <p>
                            <b>Req By:</b>
                            Client @Html.CheckBox("clientreq", Model.clientreq != null && (bool)Model.clientreq)
                            CMC @Html.CheckBox("cmcreq", Model.cmcreq != null && (bool)Model.cmcreq)
                        </p>
                        <p>Other: @Html.TextBoxFor(m => m.otherpkg)</p>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Heating Instructions</label>
                        @Html.TextBoxFor(m => m.heatinginstructions, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-6">
                        <label>Mixing Instructions</label>
                        @Html.TextBoxFor(m => m.mixinginstructions, new { @class = "form-control" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Other Instructions</label>
                        @Html.TextBoxFor(m => m.otherhandlinginstr, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-6">
                        <label>Shipping SOP</label>
                        @Html.TextBoxFor(m => m.shippingnotes, new { @class = "form-control" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Normal Appearance</label>
                        @Html.TextBoxFor(m => m.normalappearence, new { @class = "form-control" })
                    </div>
                    <div class="form-group col-md-6">
                        <label>Rejection Criterion</label>
                        @Html.TextBoxFor(m => m.rejectioncriterion, new { @class = "form-control" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-3">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <th style="background-color: yellow">Right to Know</th>
                                    <th style="text-align: center">@Html.CheckBox("righttoknow", Model.righttoknow != null && (bool)Model.righttoknow)</th>
                                </tr>
                                <tr>
                                    <th style="background-color: yellow">Fire List</th>
                                    <th style="text-align: center">@Html.CheckBox("firelist", Model.firelist != null && (bool)Model.firelist)</th>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="form-group col-md-3">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <th style="background-color: yellow">Freezable List</th>
                                    <th style="text-align: center">@Html.CheckBox("freezablelist", Model.freezablelist != null && (bool)Model.freezablelist)</th>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="form-group col-md-3">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <th style="background-color: yellow">SARA</th>
                                    <th style="text-align: center">@Html.CheckBox("sara", Model.sara != null && (bool)Model.sara)</th>
                                </tr>
                                <tr>
                                    <th style="background-color: green; color: white">SARA 313</th>
                                    <th style="text-align: center">@Html.CheckBox("sara313", Model.sara313 != null && (bool)Model.sara313)</th>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="form-group col-md-3">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <th style="background-color: purple; color: white">SARA 302</th>
                                    <th style="text-align: center">@Html.CheckBox("sara302ehs", Model.sara302ehs != null && (bool)Model.sara302ehs)</th>
                                </tr>
                                <tr>
                                    <th style="background-color: red; color: white">EPA Biocide</th>
                                    <th style="text-align: center">@Html.CheckBox("epabiocide", Model.epabiocide != null && (bool)Model.epabiocide)</th>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#health"><i class="fa fa-suitcase fa-fw"></i> Health & Safety</h3>
            </div>
            <div id="health" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>Equipment</label>
                        <p>@Html.CheckBox("hood", Model.hood != null && (bool)Model.hood) Hood</p>
                        <p>@Html.CheckBox("safetyglasses", Model.safetyglasses != null && (bool)Model.safetyglasses) Safety Glasses</p>
                        <p>@Html.CheckBox("gloves", Model.gloves != null && (bool)Model.gloves) Gloves</p>
                        <p>
                            @*@Html.TextBoxFor(m => m.glovetype, new { @class = "form-control" })*@
                            @Html.DropDownListFor(m => m.glovetype, Model.ListOfGloves, new { @class = "form-control" })
                            <!--#region DDL for Glove Types-->
                            @*<select class="form-control">
                                    <option>Glove Type</option>
                                    <option>Butyl</option>
                                    <option>Latex</option>
                                    <option>Latex/Neoprene</option>
                                    <option>Neoprene</option>
                                    <option>Nitrile</option>
                                    <option>Nitrile/Silver</option>
                                    <option>Polyvinyl</option>
                                    <option>Polyvinyl Alcohol</option>
                                    <option>Silver 4H</option>
                                    <option>Silver Shield</option>
                                    <option>Special</option>
                                    <option>Viton</option>
                                </select>*@
                            <!--#endregion-->
                        </p>
                        <p>@Html.CheckBox("booties", Model.booties != null && (bool)Model.booties) Booties</p>
                        <p>@Html.CheckBox("cleanroomequipment", Model.cleanroomequipment != null && (bool)Model.cleanroomequipment) Clean Room Equip</p>
                        <p>@Html.CheckBox("apron", Model.apron != null && (bool)Model.apron) Apron</p>
                        <p>@Html.CheckBox("armsleeves", Model.armsleeves != null && (bool)Model.armsleeves) Arm Sleeves</p>
                        <p>@Html.CheckBox("halfmaskrespirator", Model.halfmaskrespirator != null && (bool)Model.halfmaskrespirator) Half Mask Resp</p>
                        <p>@Html.CheckBox("fullfacerespirator", Model.fullfacerespirator != null && (bool)Model.fullfacerespirator) Full Face Resp</p>
                        <p>@Html.CheckBox("fullsuit", Model.fullsuit != null && (bool)Model.fullsuit) Full Suit</p>
                        <p>@Html.CheckBox("otherequipment", Model.otherequipment != null && (bool)Model.otherequipment) Other Equipment</p>
                        <p>@Html.TextBoxFor(m => m.otherequipmentdescription, new { @class = "form-control" })</p>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Health Hazards</label>
                        <p>@Html.CheckBox("toxic", Model.toxic != null && (bool)Model.toxic) Toxic</p>
                        <p>@Html.CheckBox("highlytoxic", Model.highlytoxic != null && (bool)Model.highlytoxic) Highly Toxic</p>
                        <p>@Html.CheckBox("reproductivetoxin", Model.reproductivetoxin != null && (bool)Model.reproductivetoxin) Reproductive Toxin</p>
                        <p>@Html.CheckBox("corrosivehaz", Model.corrosivehaz != null && (bool)Model.corrosivehaz) Corrosive Hazard</p>
                        <p>@Html.CheckBox("sensitizer", Model.sensitizer != null && (bool)Model.sensitizer) Sensitizer</p>
                        <p>@Html.CheckBox("carcinogen", Model.carcinogen != null && (bool)Model.carcinogen) Carcinogen</p>
                        <p>@Html.CheckBox("irritant", Model.irritant != null && (bool)Model.irritant) Irritant</p>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Routes of Entry</label>
                        <p>@Html.CheckBox("ingestion", Model.ingestion != null && (bool)Model.ingestion) Ingestion</p>
                        <p>@Html.CheckBox("inhalation", Model.inhalation != null && (bool)Model.inhalation) Inhalation</p>
                        <p>@Html.CheckBox("skincontact", Model.skincontact != null && (bool)Model.skincontact) Skin Contact</p>
                        <p>@Html.CheckBox("eyecontact", Model.eyecontact != null && (bool)Model.eyecontact) Eye Contact</p>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Physical Hazards</label>
                        <p>@Html.CheckBox("combustible", Model.combustible != null && (bool)Model.combustible) Combustible</p>
                        <p>@Html.CheckBox("corrosive", Model.corrosive != null && (bool)Model.corrosive) Corrosive</p>
                        <p>@Html.CheckBox("flammable", Model.flammable != null && (bool)Model.flammable) Flammable</p>
                        <p>@Html.CheckBox("oxidizer", Model.oxidizer != null && (bool)Model.oxidizer) Oxidizer</p>
                        <p>@Html.CheckBox("reactive", Model.reactive != null && (bool)Model.reactive) Reactive</p>
                        <p><input type="checkbox"> Toxic - (Physical Toxic) M*</p>
                        <p>@Html.CheckBox("peroxideformer", Model.peroxideformer != null && (bool)Model.peroxideformer) Peroxide Former</p>
                        <p>@Html.CheckBox("polmerizationhazard", Model.polmerizationhazard != null && (bool)Model.polmerizationhazard) Polymerization Hazard</p>
                    </div>
                </div>
                <hr>
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>HMIS Ratings</label>
                        <table class="table table-bordered">
                            <tr>
                                <th style="background-color: blue; color: white">Health</th>
                                <th style="text-align: center">@Html.TextBoxFor(m => m.health, new { @maxlength = "1", style = "border: 0px solid transparent; width: 15px;" })</th>
                            </tr>
                            <tr>
                                <th style="background-color: red; color: white">Flammability</th>
                                <th style="text-align: center">@Html.TextBoxFor(m => m.flammability, new { @maxlength = "1", style = "border: 0px solid transparent; width: 15px;" })</th>
                            </tr>
                            <tr>
                                <th style="background-color: yellow; color: gray">Reactivity</th>
                                <th style="text-align: center">@Html.TextBoxFor(m => m.reactivity, new { @maxlength = "1", style = "border: 0px solid transparent; width: 15px;" })</th>
                            </tr>
                        </table>
                    </div>
                    <div class="form-group col-md-9">
                        <label>Target Organ Effects</label>
                        <p>@Html.CheckBox("nephrotoxins", Model.nephrotoxins != null && (bool)Model.nephrotoxins) Nephrotoxins kidney damage - edema, proteinurea</p>
                        <p>@Html.CheckBox("neurotoxins", Model.neurotoxins != null && (bool)Model.neurotoxins) Neurotoxins nervous system damage - narcosis, behaviour change, decrease in motor functions</p>
                        <p>@Html.CheckBox("hepatopoetics", Model.hepatopoetics != null && (bool)Model.hepatopoetics) Hermatopoietics blood damage - cyanosis, unconsciousness</p>
                        <p>@Html.CheckBox("reporductivetoxin", Model.reporductivetoxin != null && (bool)Model.reporductivetoxin) Reproductive toxin birth defects and sterility</p>
                        <p>@Html.CheckBox("hepatotoxins", Model.hepatotoxins != null && (bool)Model.hepatotoxins) Hepatotoxins: Liver damage - jaundice, liver enlargement</p>
                        <p>@Html.CheckBox("pulmonarydisfunction", Model.pulmonarydisfunction != null && (bool)Model.pulmonarydisfunction) Pulmonary disfunctions: Lung damage, shortness of breath, chest tightness, cough</p>
                        <p>@Html.CheckBox("cutaneoushazards", Model.cutaneoushazards != null && (bool)Model.cutaneoushazards) Cutaneous hazards - skin damage, rashes, defatting of skin</p>
                        <p>@Html.CheckBox("eyehazards", Model.eyehazards != null && (bool)Model.eyehazards) Eye hazards: Impaired vision, conjunctivitis, corneal damage</p>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#ghs"><i class="fa fa-suitcase fa-fw"></i> GHS Information</h3>
            </div>
            <div id="ghs" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>P & H Statements</label>
                        <input class="form-control" placeholder="Search for statements"><br />
