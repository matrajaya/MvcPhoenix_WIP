            <div class="col-md-6">
                @*<button class="btn btn-success btn-block">Save Changes</button>*@
                <input type="submit" class="btn btn-success btn-block" value="Save Changes" />
            </div>
            <div class="col-md-6">
                @*<button class="btn btn-danger btn-block">De-Activate</button>*@
                @Ajax.ActionLink("De-Activate", "DeActivateProductMaster", "Products", new { id = Model.productmasterid }, new AjaxOptions
                             {
                                 UpdateTargetId = "dvSaveResult",
                                 InsertionMode = InsertionMode.Replace,
                                 HttpMethod = "GET",
                                 OnSuccess = "$('#discontinued').prop('checked',true)"
                             }, new { @class = "btn btn-danger btn-block" })
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-lg-12">
                <span><i class="fa fa-arrow-circle-up"></i> <a href="#top">Back to Top</a></span>
                <span class="pull-right"><a href="#top">Back to Top</a> <i class="fa fa-arrow-circle-up"></i></span>
            </div>
        </div>

        <div class="form-group row center-block">
            <div class="col-md-12 text-center" id="dvspinner" style="display:none; font-weight:bold">
                <img src="~/Content/images/ani-rebuild.gif" /> Saving Profile
            </div>

            <div id="dvSaveResult" class="col-md-12 text-center" style="font-weight:bold">
            </div>
        </div>
    }
</div>
