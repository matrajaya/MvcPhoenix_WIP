@model MvcPhoenix.Models.ProductProfile
@{
    ViewBag.Title = "Product Profiles";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            @*<img src="~/Content/images/logos/oltlogo.jpg" style="width: auto; height: 73px; float: right" alt="acme logo">*@
            <img src="@Model.logofilename" style="width: auto; height: 73px; float: right" alt="No Logo for this client">
        </div>
        <div class="col-lg-12">
            <h3 class="page-header">
                Product Profile <small>(ID= @Model.productmasterid.ToString())</small>
                <a href="~/Products/Index" class="btn btn-primary btn-xs" style="margin-left:50px;">&larr; Back to landing page</a>
            </h3>
        </div>
    </div>

