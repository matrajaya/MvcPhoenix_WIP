                    <div class="clearfix"></div>
                    <br />

                    <div class="col-md-12">
                        <a href="#" class="btn btn-primary btn-xs" onclick="alert('Build new object, Modal edit popup')">Add a Size</a>
                        <table class="table table-condensed table-hover table-bordered" style="font-size:.85em;">
                            <tr>
                                <th colspan="13" style="text-align:center;">Product is available in the following sizes</th>
                            </tr>
                            <tr>
                                <th>Size</th>
                                <th>Mnemonic</th>
                                <th>BusArea</th>
                                <th>Warehouse</th>
                                <th>Bin</th>
                                <th>Min Qty</th>
                                <th>Max Qty</th>
                                <th>Re-order Qty</th>
                                <th>Package</th>
                                <th>Gnd Haz</th>
                                <th>Air Hzd</th>
                                <th>Notes</th>
                                <th>Action</th>
                            </tr>
                            @foreach (var item in Model.ListOfShelfItems)
                            {
                                <tr>
                                    <td><strong>@item.size</strong></td>
                                    <td>@item.mnemonic</td>
                                    <td>@item.busarea</td>
                                    <td>@item.warehouse</td>
                                    <td>@item.bin</td>
                                    <td>@item.reordermin.ToString()</td>
                                    <td>@item.reordermax.ToString()</td>
                                    <td>@item.reorderqty.ToString()</td>
                                    <td>@item.package</td>
                                    <td>@item.groundhazard</td>
                                    <td>@item.airhazard</td>
                                    <td>@item.notes</td>
                                    <td><a href="#" onclick="alert('Build object, Modal edit popup')">Edit @item.shelfid.ToString()</a></td>
                                </tr>
                            }
                        </table>


                    </div>



                </div>
            </div>
        </div>


