<script type="text/javascript">
    function LookupUNGround() {
        var myurl = '@Url.Action("LookupUNGround", "Products")';
        $.getJSON(myurl,
                    { id: $("#grnunnumber").val() },
                    function (jsonobj) {
                        $("#grnpkgrp").val(jsonobj.packinggroup);
                        $("#grnhazsubcl").val(jsonobj.subclass);
                        $("#grnlabel").val(jsonobj.labelreq);
                        $("#grnsublabel").val(jsonobj.subsidlabelreq);
                        $("#grnhazcl").val(jsonobj.hazardclass);
                        $("#grnshipname").val(jsonobj.propershippingname);
                        $("#grnosname").val(jsonobj.nosname);
                    });
    }

    function LookupUNAir() {
        var myurl = '@Url.Action("LookupUNAir", "Products")';
        $.getJSON(myurl,
                    { id: $("#airunnumber").val() },
                    function (jsonobj) {
                        $("#airpkgrp").val(jsonobj.packinggroup);
                        $("#airhazsubcl").val(jsonobj.subclass);
                        $("#airlabel").val(jsonobj.labelreq);
                        $("#airsublabel").val(jsonobj.subsidlabelreq);
                        $("#airhazcl").val(jsonobj.hazardclass);
                        $("#airshipname").val(jsonobj.propershippingname);
                        $("#airosname").val(jsonobj.nosname);
                    });
    }

    function LookupUNSea() {
        var myurl = '@Url.Action("LookupUNSea", "Products")';
        $.getJSON(myurl,
                    { id: $("#seaunnum").val() },
                    function (jsonobj) {
                        $("#seapkgrp").val(jsonobj.packinggroup);
                        $("#seahazsubcl").val(jsonobj.subclass);
                        $("#sealabel").val(jsonobj.labelreq);
                        $("#seasublabel").val(jsonobj.subsidlabelreq);
                        $("#seahazcl").val(jsonobj.hazardclass);
                        $("#seashipname").val(jsonobj.propershippingname);
                        $("#seanosname").val(jsonobj.nosname);
                    });
    }
</script>
