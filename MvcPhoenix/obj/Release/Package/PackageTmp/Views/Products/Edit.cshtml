@model MvcPhoenix.Models.ProductProfile
@{
    ViewBag.Title = "Product Profile:";
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <img src="@Model.logofilename" style="width: auto; height: 73px; float: right" alt="@Model.clientname logo">
        </div>
        <div class="col-lg-12">
            <h1 class="page-header">
                @ViewBag.Title <small>@Model.productcode - @Model.productname</small>
            </h1>
        </div>
    </div>
    <!-- /.Page Heading -->
    @using (Ajax.BeginForm("SaveProductProfile", "Products",
    new AjaxOptions
    {
        OnBegin = "$('#dvSaveResult').html('')",
        HttpMethod = "POST",
        LoadingElementId = "dvspinner"
    }))
    {

    <!-- do not remove -->
        @Html.HiddenFor(m => m.productmasterid)
        @Html.HiddenFor(m => m.clientid)
        @Html.HiddenFor(m => m.productdetailid)

        <div class="form-group row">
            <div class="col-md-12">
                <i class="fa fa-arrow-circle-left"></i> @Html.ActionLink("Back to Products Index", "Index")
                <span class="pull-right label label-info">Product ID: @Model.productdetailid</span>
            </div>
            <div class="col-md-12">
                <div class="pull-right">
                    <a href="@Url.Action("PrintProfile", "Products", new { id = @Model.productdetailid })" target="_blank">Print View <i class="fa fa-file-pdf-o fa-lg fa-pull-right fa-border"></i></a>
                    @*@Html.ActionLink("Print Document", "PrintProfile", new { id = @Model.productdetailid }, new { target = "_blank" })*@
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#prodid">
                    <i class="fa fa-suitcase fa-fw"></i> Product Identification
                </h3>
            </div>
            <div id="prodid" class="panel-body collapse in">
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Client</label>
                        @Html.TextBoxFor(m => m.clientname, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group col-md-3">
                        <label>Division</label>
                        @Html.DropDownListFor(m => m.masterdivisionid, Model.ListOfDivisions, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.masterdivisionid)
                    </div>
                    <div class="form-group col-md-3">
                        <label>Business Unit</label>
                        @Html.TextBoxFor(m => m.busarea, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.busarea)
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>Equivalents</label><br />
                        <textarea rows="3" class="form-control" readonly="readonly">
                            @for (int i = 0; i < Model.ListOfEquivalents.Count(); i++)
                            {
                                @Model.ListOfEquivalents[i];
                            }
                        </textarea>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Cross Reference List</label><br />
                        <textarea rows="3" class="form-control" readonly="readonly">
                            @for (int i = 0; i < Model.ListOfProductCodesXRefs.Count(); i++)
                            {
                                @Model.ListOfProductCodesXRefs[i];
                            }
                        </textarea>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Profile Revision Date</label>
                        @Html.TextBoxFor(m => m.sgrevisiondate, "{0:dd-MMM-yyyy}", new { @class = "form-control", @readonly = "readonly" })
                        @Html.ValidationMessageFor(m => m.sgrevisiondate)
                    </div>
                    <div class="form-group col-md-3">
                        <label>MSDS Revision Date</label>
                        @Html.TextBoxFor(m => m.msdsrevisiondate, "{0:dd-MMM-yyyy}", new { @class = "form-control", @readonly = "readonly" })
                    </div>
                    <div class="form-group col-md-6">
                        <ul class="list-unstyled list-3-col">
                            <li>
                                @Html.CheckBox("globalproduct", Model.globalproduct != null && (bool)Model.globalproduct, new { @class = "checkbox-inline" })
                                <label class="control-label">Global Product</label>
                            </li>
                            <li>
                                @Html.CheckBox("accuracyverified", Model.accuracyverified != null && (bool)Model.accuracyverified, new { @class = "checkbox-inline" })
                                <label class="control-label">Accuracy Verified</label>
                            </li>
                            <li>
                                @Html.CheckBox("discontinued", Model.discontinued != null && (bool)Model.discontinued, new { @class = "checkbox-inline" })
                                <label class="control-label">Discontinued</label>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Product Code</label>
                        @Html.TextBoxFor(m => m.productcode, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.productcode)
                    </div>
                    <div class="form-group col-md-6">
                        <label>Product Name</label>
                        @Html.TextBoxFor(m => m.productname, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.productname)
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="form-group col-md-12">
                        <div id="">
                            @{Html.RenderAction("ShelfMasterList", "ShelfMaster", new { id = @Model.productdetailid });}
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#special"><i class="fa fa-suitcase fa-fw"></i> Special Handling & Storage</h3>
            </div>
            <div id="special" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-6">
                        <ul class="list-unstyled list-2-col">
                            <li>@Html.CheckBox("cleanroomgmp", Model.cleanroomgmp != null && (bool)Model.cleanroomgmp) Clean Room GMP</li>
                            <li>@Html.CheckBox("nitrogenblanket", Model.nitrogenblanket != null && (bool)Model.nitrogenblanket) Nitrogen Blanket</li>
                            <li>@Html.CheckBox("moisturesensitive", Model.moisturesensitive != null && (bool)Model.moisturesensitive) Moisture Sensitive</li>
                            <li>@Html.CheckBox("donotpackabove60", Model.donotpackabove60 != null && (bool)Model.donotpackabove60) Don't Pack Above 60% RH</li>
                            <li>@Html.CheckBox("specialblend", Model.specialblend != null && (bool)Model.specialblend) Special Blend</li>
                            <li>@Html.CheckBox("freezerstorage", Model.freezerstorage != null && (bool)Model.freezerstorage) Freezer Storage</li>
                            <li>@Html.CheckBox("refrigerate", Model.refrigerate != null && (bool)Model.refrigerate) Refrig Storage</li>
                            <li>
                                Torque: @Html.TextBoxFor(m => m.torquerequirements, new { @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.torquerequirements)
                            </li>
                            <li>
                                Flash Point: @Html.TextBoxFor(m => m.flashpoint, new { @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.flashpoint)
                            </li>
                        </ul>
                    </div>

                    <div class="form-group col-md-6">
                        <ul class="list-unstyled list-2-col">
                            <li>@Html.CheckBox("freezablelist", Model.freezablelist != null && (bool)Model.freezablelist) Freezable List</li>
                            <li>@Html.CheckBox("flammableground", Model.flammableground != null && (bool)Model.flammableground) Flammable Ground</li>
                            <li>@Html.CheckBox("flammablestorageroom", Model.flammablestorageroom != null && (bool)Model.flammablestorageroom) Flammable Storage</li>
                            <li>@Html.CheckBox("heatpriortofilling", Model.heatpriortofilling != null && (bool)Model.heatpriortofilling) Heat Prior to Filing</li>
                            <li>@Html.TextBoxFor(m => m.heatinginstructions, new { @class = "form-control", @placeholder = "Heating Instructions" })</li>
                            <li>@Html.CheckBox("mixwell", Model.mixwell != null && (bool)Model.mixwell) Mix Well</li>
                            <li>@Html.TextBoxFor(m => m.mixinginstructions, new { @class = "form-control", @placeholder = "Mixing Instructions" })</li>
                            <li>
                                <b>Req By:</b>
                                Client @Html.CheckBox("clientreq", Model.clientreq != null && (bool)Model.clientreq)
                                CMC @Html.CheckBox("cmcreq", Model.cmcreq != null && (bool)Model.cmcreq)
                            </li>
                            <li>
                                Other: @Html.TextBoxFor(m => m.otherpkg, new { @class = "form-control" })
                                @Html.ValidationMessageFor(m => m.otherpkg)
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Other Instructions</label>
                        @Html.TextAreaFor(m => m.otherhandlinginstr, new { @class = "form-control", @rows = 1 })
                        @Html.ValidationMessageFor(m => m.otherhandlinginstr)
                    </div>
                    <div class="form-group col-md-3">
                        <label>Harmonized Code</label>
                        @Html.DropDownListFor(m => m.harmonizedcode, Model.ListOfHarmonizedCodes, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.harmonizedcode)
                    </div>
                    <div class="form-group col-md-2">
                        <label>Customs Value</label>
                        @Html.TextBoxFor(m => m.customsvalue, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.customsvalue)
                    </div>
                    <div class="form-group col-md-1">
                        <label class="invisible">.</label>
                        @Html.TextBoxFor(m => m.customsvalueunit, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Shipping SOP</label>
                        @Html.TextAreaFor(m => m.shippingnotes, new { @class = "form-control", @rows = 1 })
                        @Html.ValidationMessageFor(m => m.shippingnotes)
                    </div>
                    <div class="form-group col-md-4">
                        <label>End Use</label>
                        @Html.DropDownListFor(m => m.enduse, Model.ListOfEndUsesForCustoms, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.enduse)
                    </div>
                    <div class="form-group col-md-2">
                        <label>Cease Ship </label>
                        @Html.TextBoxFor(m => m.ceaseshipdifferential, new { @class = "form-control", @readonly = "readonly" })
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-12">
                        <label>Rejection Criterion</label>
                        @Html.TextAreaFor(m => m.rejectioncriterion, new { @class = "form-control", @rows = 1 })
                        @Html.ValidationMessageFor(m => m.rejectioncriterion)
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#msds"><i class="fa fa-suitcase fa-fw"></i> MSDS Information</h3>
            </div>
            <div id="msds" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>MSDS Revision Date</label>
                        @Html.TextBoxFor(m => m.msdsrevisiondate, "{0:dd-MMM-yyyy}", new { @class = "date-picker form-control" })
                        @Html.ValidationMessageFor(m => m.msdsrevisiondate)
                    </div>
                    <div class="form-group col-md-3">
                        <label>MSDS Revision Number</label>
                        @Html.TextBoxFor(m => m.msdsrevisionnumber, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.msdsrevisionnumber)
                    </div>
                    <div class="form-group col-md-3">
                        <label>MSDS Other Number</label>
                        @Html.TextBoxFor(m => m.msdsothernumber, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.msdsothernumber)
                    </div>
                    <div class="form-group col-md-3">
                        <label style="color: white">.</label>
                        <button type="button" class="btn btn-info btn-block" onclick="alert('TODO Build, TODO')">Open SDS PDF</button>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>SDS Contact Name</label>
                        @Html.TextBoxFor(m => m.sdscontactname, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.sdscontactname)
                    </div>
                    <div class="form-group col-md-6">
                        <label>SDS Contact Phone</label>
                        @Html.TextBoxFor(m => m.sdscontactphone, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.sdscontactphone)
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>China Test Certification Date</label>
                        @Html.TextBoxFor(m => m.chinacertificationdate, "{0:dd-MMM-yyyy}", new { @class = "date-picker form-control" })
                        @Html.ValidationMessageFor(m => m.chinacertificationdate)
                    </div>
                    <div class="form-group col-md-3">
                        <label>Label Contact Name</label>
                        @Html.TextBoxFor(m => m.labelcontactname, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.labelcontactname)
                    </div>
                    <div class="form-group col-md-3">
                        <label>Label Contact Phone</label>
                        @Html.TextBoxFor(m => m.labelcontactphone, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.labelcontactphone)
                    </div>
                    <div class="form-group col-md-3">
                        <label>Technical Sheet Available</label>
                        <p>@Html.CheckBox("technicalsheet", Model.technicalsheet != null && (bool)Model.technicalsheet)</p>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-2">
                        <label>Shelf Life</label>
                        @Html.TextBoxFor(m => m.shlflife, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.shlflife)
                    </div>
                    <div class="form-group col-md-2">
                        <label>Hazard Class</label>
                        @Html.TextBoxFor(m => m.hazardclassground_sg, new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.hazardclassground_sg)
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#ghs"><i class="fa fa-suitcase fa-fw"></i> GHS Information*- Work In Progress</h3>
            </div>
            <div id="ghs" class="panel-body collapse">
                <div class="row">
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Signal Word</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Danger</option>
                                <option>Warning</option>
                                <option>Caution</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Symbol 1</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Explosive</option>
                                <option>Flammable</option>
                                <option>Oxidizing</option>
                                <option>Compressed Gas</option>
                                <option>Corrosive</option>
                                <option>Toxic</option>
                                <option>Irritant</option>
                                <option>Health Hazard</option>
                                <option>Environmental Hazard</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Symbol 2</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Explosive</option>
                                <option>Flammable</option>
                                <option>Oxidizing</option>
                                <option>Compressed Gas</option>
                                <option>Corrosive</option>
                                <option>Toxic</option>
                                <option>Irritant</option>
                                <option>Health Hazard</option>
                                <option>Environmental Hazard</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Symbol 3</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Explosive</option>
                                <option>Flammable</option>
                                <option>Oxidizing</option>
                                <option>Compressed Gas</option>
                                <option>Corrosive</option>
                                <option>Toxic</option>
                                <option>Irritant</option>
                                <option>Health Hazard</option>
                                <option>Environmental Hazard</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Symbol 4</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Explosive</option>
                                <option>Flammable</option>
                                <option>Oxidizing</option>
                                <option>Compressed Gas</option>
                                <option>Corrosive</option>
                                <option>Toxic</option>
                                <option>Irritant</option>
                                <option>Health Hazard</option>
                                <option>Environmental Hazard</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-md-2">
                        <div class="form-group">
                            <label>GHS Symbol 5</label>
                            <select class="form-control">
                                <option> </option>
                                <option>Explosive</option>
                                <option>Flammable</option>
                                <option>Oxidizing</option>
                                <option>Compressed Gas</option>
                                <option>Corrosive</option>
                                <option>Toxic</option>
                                <option>Irritant</option>
                                <option>Health Hazard</option>
                                <option>Environmental Hazard</option>
                            </select>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="row">
                    <div class="form-group col-md-3">
                        <label>P & H Statements</label>
                        <input class="form-control" placeholder="Search for statements"><br />
                        <button type="button" class="btn btn-info btn-block">Update P&H Data</button>
                    </div>
                    <div class="form-group table-responsive col-md-9">
                        <table class="table table-striped">
                            <tbody>
                                <tr>
                                    <th></th>
                                    <th>Label Exclude</th>
                                    <th>P & H Code</th>
                                    <th>Language</th>
                                    <th>Statement</th>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                    <td><input type="checkbox"></td>
                                    <td>H226</td>
                                    <td>EN</td>
                                    <td>IF ON SKIN (or hair): Remove/take off immediately all contaminated clothing. Rinse skin with water/shower.</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                    <td><input type="checkbox"></td>
