@model MvcPhoenix.Models.ProductProfile
@{
    ViewBag.Title = "Product Profiles";
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <img src="@Model.logofilename" style="width: auto; height: 73px; float: right" alt="Logo Missing">
        </div>
        <div class="col-lg-12">
            <h1 class="page-header">
                @ViewBag.Title <small>Sample Guide</small>
            </h1>
        </div>
    </div>
    <!-- /.Page Heading -->

    <form role="form">
        <div class="row">
            <div class="form-group col-md-12">
                <div class="input-group">
                    <span class="input-group-addon"><i class="fa fa-search fa-fw"></i></span>
                    <input type="search" class="form-control" placeholder="Type Product Code...">
                    <span class="input-group-btn"><input type="submit" class="btn btn-success" value="Search" /></span>
                </div>
                <hr />
            </div>
        </div>

        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title" data-toggle="collapse" data-target="#prodid"><i class="fa fa-suitcase fa-fw"></i> Product Identification</h3>
            </div>
            <div id="prodid" class="panel-body">
                <div class="row">
                    <label id="discontinued" class="col-md-12 collapse form-control" style="background-color: red; color: white"> Discontinued Product</label>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Client</label>
                        <select class="form-control">
                            <option>Select Client</option>
                            <option>Akzo Nobel</option>
                            <option>Clariant</option>
                            <option>DSM</option>
                            <option>Elementis</option>
                            <option>Evonik</option>
                            <option>Grace</option>
                            <option>Lonza</option>
                            <option>OMG</option>
                            <option>Stepan</option>
                            <option>Thor</option>
                            <option>Wacker</option>
                        </select>
                    </div>
                    <div class="form-group col-md-6">
                        <label>Business Unit</label>
                        <select class="form-control">
                            <option></option>
                            <option>Coatings</option>
                            <option>Epoxy</option>
                            <option>Paint</option>
                            <option>Rubber
                            <option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Cross Reference List</label><br />
                        <textarea rows="5" class="form-control"></textarea>
                    </div>
                    <div class="form-group col-md-3">
                        <label>Profile Revision Date</label>
                        <input type="datetime-local" class="form-control" disabled>
                    </div>
                    <div class="form-group col-md-3">
                        <label>MSDS Revision Date</label>
                        <input type="datetime-local" class="form-control" disabled>
                    </div>
                    <div class="form-group  col-md-3">
                        <label>Label Revision Date</label>
                        <input type="datetime-local" class="form-control" disabled>
                    </div>
                    <div class="form-group col-md-3">
                        <label><input type="checkbox" value=""> Global Product</label><br />
                        <label><input type="checkbox" value=""> Accuracy Verified</label><br />
                        <label data-toggle="collapse" data-target="#discontinued"><input type="checkbox" value=""> Discontinued</label>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-6">
                        <label>Product Code</label>
                        <input class="form-control">
                    </div>
                    <div class="form-group col-md-6">
                        <label>Product Name</label>
                        <input class="form-control">
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-md-2">
                        <label>Equivalence</label>
                        <input class="form-control">
                    </div>
                    <div class="form-group col-md-2">
                        <label>Shelf Life</label>
                        <input class="form-control">
                    </div>
                    <div class="form-group col-md-2">
                        <label>Hazard Class</label>
                        <input class="form-control">
                    </div>
                    <div class="form-group col-md-2">
