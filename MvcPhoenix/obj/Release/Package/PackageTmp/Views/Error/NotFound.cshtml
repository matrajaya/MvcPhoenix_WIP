@{
    ViewBag.Title = "Resource Not Found";
    Layout = "~/Views/Shared/_LayoutNaked.cshtml";
}

<div class="container">
    <div class="col-md-4 col-md-offset-4">
        <div class="row">
            <h1>@ViewBag.Title.</h1>
            <hr />
        </div>
        <div class="row text-center">
            <div class="form-group">
                <h2>Hmmmmm...</h2>
                <p>You went where you shouldn't and now you have 2 choices.</p>
            </div>
        </div>

        <div class="row">
            <div class="form-group ">
                <div class="col-md-5">
                    <input type="button" onclick="goBack()" value="Go Back" class="btn btn-primary btn-block pull-left" />
                </div>
                <div class="col-md-2 text-center">
                    <p>OR</p>
                </div>
                <div class="col-md-5">
                    <a href="/Home/Index" class="btn btn-info btn-block pull-right">Go Home <i class="fa fa-home"></i></a>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
</div>